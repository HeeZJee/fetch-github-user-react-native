{
  "expo": {
    "name": "githubUsers",
    "slug": "githubUsers",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.heezjee.githubUsers"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "A simplest way get github users info.",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "android": {
      "package": "com.heezjee.githubUsers"
    },
    "entryPoint": "node_modules/expo/AppEntry.js"
  }
}
